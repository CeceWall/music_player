{
  "name": "music-player",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-slider": "^9.7.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "rc-progress": "^2.1.1",
    "rc-slider": "^7.0.1",
    "rc-tooltip": "^3.4.3",
    "react": "^15.5.4",
    "react-document-title": "^2.0.3",
    "react-dom": "^15.5.4",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.5"
  },
  "scripts": {
    "build-css": "scss --update src:src",
    "watch-css": "scss --watch src:src",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "build": "npm run build-css && react-scripts build && cp musics.json build/musics.json",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
